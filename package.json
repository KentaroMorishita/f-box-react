{
  "name": "f-box-react",
  "version": "0.1.5",
  "description": "React hooks and utilities for working with f-box-core.",
  "type": "module",
  "files": [
    "dist/types",
    "dist/index.*"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/main.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "publish:public": "npm publish --access public",
    "prepare": "npm run build"
  },
  "keywords": [
    "typescript",
    "react",
    "hooks",
    "react-hooks",
    "react-hooks-library",
    "f-box",
    "f-box-core",
    "functional-programming",
    "functor",
    "applicative",
    "monad",
    "maybe",
    "either",
    "task",
    "async",
    "box",
    "reactive",
    "reactive-programming",
    "state-management",
    "library"
  ],
  "author": "Kentaro Morishita",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KentaroMorishita/f-box-react"
  },
  "peerDependencies": {
    "f-box-core": ">=0.1.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitest/coverage-v8": "^2.1.5",
    "f-box-core": "^0.1.9",
    "jsdom": "^25.0.1",
    "react": "^18.3.1",
    "typescript": "~5.6.3",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.5"
  }
}
